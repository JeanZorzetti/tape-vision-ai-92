{
  "nelogica": {
    "development": {
      "clientId": "your_dev_client_id_here",
      "clientSecret": "your_dev_client_secret_here",
      "username": "your_dev_username_here",
      "password": "your_dev_password_here",
      "apiKey": "your_dev_api_key_here"
    },
    "production": {
      "clientId": "your_prod_client_id_here",
      "clientSecret": "your_prod_client_secret_here",
      "username": "your_prod_username_here",
      "password": "your_prod_password_here",
      "apiKey": "your_prod_api_key_here"
    }
  },
  "jwt": {
    "secret": "your_very_secure_jwt_secret_key_here_at_least_256_bits",
    "refreshSecret": "your_very_secure_refresh_secret_key_here",
    "expiresIn": "1h",
    "refreshExpiresIn": "7d"
  },
  "encryption": {
    "algorithm": "aes-256-gcm",
    "secretKey": "your_32_character_secret_key_here",
    "ivLength": 16
  },
  "database": {
    "mongodb": {
      "development": {
        "username": "dev_user",
        "password": "dev_password",
        "authSource": "admin"
      },
      "production": {
        "username": "prod_user",
        "password": "secure_prod_password_here",
        "authSource": "admin"
      }
    },
    "redis": {
      "development": {
        "password": null
      },
      "production": {
        "password": "secure_redis_password_here"
      }
    }
  },
  "external": {
    "email": {
      "smtp": {
        "user": "your_smtp_username",
        "pass": "your_smtp_password"
      }
    },
    "webhook": {
      "slack": {
        "url": "https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK",
        "token": "your_slack_bot_token"
      },
      "discord": {
        "webhookUrl": "https://discord.com/api/webhooks/YOUR/WEBHOOK/URL"
      }
    },
    "cloudStorage": {
      "aws": {
        "accessKeyId": "your_aws_access_key_id",
        "secretAccessKey": "your_aws_secret_access_key",
        "region": "us-east-1"
      }
    }
  },
  "monitoring": {
    "sentryDsn": "https://your-sentry-dsn-here@sentry.io/project-id",
    "datadogApiKey": "your_datadog_api_key_here",
    "newRelicLicenseKey": "your_new_relic_license_key_here"
  },
  "security": {
    "rateLimitRedisKey": "rate_limit_key_here",
    "sessionSecret": "your_session_secret_here",
    "csrfSecret": "your_csrf_secret_here"
  }
}