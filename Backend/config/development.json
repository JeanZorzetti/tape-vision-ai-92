{
  "server": {
    "port": 3001,
    "host": "localhost",
    "cors": {
      "origin": ["http://localhost:8080", "http://localhost:3000", "http://127.0.0.1:8080"],
      "credentials": true
    }
  },
  "trading": {
    "enabled": false,
    "maxDailyLoss": 100,
    "maxPositionSize": 1,
    "confidenceThreshold": 0.8,
    "emergencyStopEnabled": true
  },
  "ai": {
    "analysisSettings": {
      "confidenceThreshold": 0.8,
      "maxSignalsPerHour": 20,
      "enablePatternRecognition": true,
      "processingTimeout": 10000,
      "memoryLimit": 256
    }
  },
  "database": {
    "mongodb": {
      "uri": "mongodb://localhost:27017/tape-vision-ai-dev",
      "options": {
        "maxPoolSize": 5
      }
    },
    "redis": {
      "host": "localhost",
      "port": 6379,
      "db": 1,
      "keyPrefix": "tape-vision-dev:"
    }
  },
  "nelogica": {
    "environment": "demo",
    "autoReconnect": true,
    "timeout": 10000
  },
  "logging": {
    "level": "debug",
    "file": {
      "enabled": false
    },
    "console": {
      "enabled": true,
      "colorize": true,
      "timestamp": true
    }
  },
  "performance": {
    "maxLatency": 50,
    "healthCheckInterval": 10000
  }
}